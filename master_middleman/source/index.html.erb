<div class="main-container">
  <link href="https://styleguide-v13.cfapps.io/app.css" type="text/css" rel="stylesheet"/>
  <style>
  body {
    overflow-y: inherit;
    background-color: #f8f8f8;
  }
  a, .link {
    border-bottom: none;
  }
  .to-top a,
  a#top {
    display: none;
  }
  #container-main.container {
    max-width: 1260px;
  }
  .sidebar {
    display: none;
  }
  ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
    color: rgba(0,0,0,0.8);
  }
  ::-moz-placeholder { /* Firefox 19+ */
    color: rgba(0,0,0,0.8);
  }
  :-ms-input-placeholder { /* IE 10+ */
    color: rgba(0,0,0,0.8);
  }
  :-moz-placeholder { /* Firefox 18- */
    color: rgba(0,0,0,0.8);
  }
  form#search-home {
    margin: 0 auto 60px;
    max-width: 760px;
  }
  form#search-home input {
    height: 50px;
    background-image: url(https://d1fto35gcfffzn.cloudfront.net/images/docs/md-search.svg);
    background-repeat: no-repeat;
    background-position: 10px 12px;
    font-size: 18px;
    padding-left: 40px;
  }
  #pcf-landing {
    margin-top: 40px;
  }
  @media(max-width: 699px) {
    #pcf-landing {
      margin-top: 0;
    }
  }
  .container {
    background-color: #f8f8f8;
    max-width: none;
  }
  .external-link:after {
    content: none;
  }
  .docs-home-panel  {
    background-color: white;
    display: flex;
    flex-direction: column;

    box-shadow: none;
    margin: 0;
  }
  .docs-home-panel .logo-icon img {
    max-height: 4.8em;
  }
  .content a[href^="http"]:after {
    display: none;
  }
  .content a:focus,
  .content a:active,
  .content a:hover {
    border-bottom-style: none;
  }
  .content a:focus,
  .content a:active {
    background-color: transparent;
  }
  .content a:focus {
    text-decoration: none;
  }
  .container img {
    max-width: 6.4em;
    width: 100%;
  }
  .docs-link-panel {
    height: 64px;
  }
  .docs-link-panel:hover  {
    border-bottom: none;
  }
  .docs-link-panel > div {
    margin: 0 auto;
  }
  .content h6,
  .content .h6 {
    font-size: .9em;
  }
  .content .type-brand-8 {
    color: #314351 !important;
  }
  .content .type-accent-5 {
    color: #0065d5 !important;
  }

  @media(max-width:1200px) {
    #container-main.container {
     max-width: 960px;
    }
  }

  @media(max-width:768px) {
    #container-main.container {
     max-width: 500px;
    }
  }

  a, .link, .quick-links a {
  color: #1D428A;
  border-bottom: none;
  font-weight: 600;
  }

  .btn.btn-default {
  background-color: #1D428A;
  border-color: #1D428A;
  }

  @media(max-width:993px) {
